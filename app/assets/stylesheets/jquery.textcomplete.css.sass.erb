.dropdown-menu
  :border 1px solid #ddd
  :background-color white

  li
    :border-top 1px solid #ddd
    :padding 2px 5px

  li:first-child
    :border-top none
  li:hover
  .active
    :background-color rgb(110, 183, 219)

  :list-style none
  :padding 0
  :margin 0
  :margin-left 50px

  a:hover
    :cursor pointer
  img
    :width 1em

.textcomplete-wrapper
  :text-align left
